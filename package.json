{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.223",
    "gatsby-link": "^1.6.38",
    "gatsby-plugin-google-analytics": "^1.0.24",
    "gatsby-plugin-react-helmet": "^2.0.6",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.3",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": " gatsby build --prefix-paths",
    "postbuild": "npm run clean && mv public/**/* .",
    "start": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "test": "jest",
    "clean": "rimraf src jest public"
  },
  "devDependencies": {
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "setupFiles": [
      "./jest/mocks.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec|test))\\.jsx?$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/\\.cache/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
