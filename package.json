{
  "name": "personnal_gh_page_site",
  "description": "personnal github page site",
  "version": "1.0.0",
  "author": "Jin Xu <erlking@qq.com>",
  "private": true,
  "dependencies": {
    "gatsby": "^2.0.19",
    "gatsby-link": "^2.0.4",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gh-pages": "^2.0.1",
    "jest": "^23.6.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0"
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": " gatsby build --prefix-paths && mv ./public/* ./",
    "postbuild": "npm run clean",
    "start": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,json,md}\"",
    "test": "jest",
    "lint": "node_modules/.bin/eslint .",
    "clean": "rimraf src jest public"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "setupFiles": [
      "./jest/mocks.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(spec|test))\\.jsx?$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/\\.cache/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
